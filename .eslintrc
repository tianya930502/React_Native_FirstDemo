{
  "extends": "airbnb",
  "globals": {
    "window": true,
    "URLSearchParams": true,
    "navigator": true,
    "localStorage":true,
    "sessionStorage": true,
    "document": true,
    "__CDN__": false,
    "location":true,
    "history":true,
    "FormData":true
  },
  "rules": {
    "jsx-a11y/href-no-hash":[0],
    "no-multi-spaces":[0],
    "dot-notation":[0],
    "react/require-default-props":[0],
    "react/sort-comp":[0],
    "react/jsx-boolean-value":[0],
    "spaced-comment":[0],
    "no-return-assign":[0],
    "no-plusplus":[0],
    "no-param-reassign":[0],
    "no-unused-expressions":[0],
    "class-methods-use-this":[0],
    "no-eval":[0],
    "no-mixed-operators":[0],
    "no-useless-constructor":[0],
    "guard-for-in":[0],
    "no-unused-vars":[1],
    "react/no-multi-comp":[0],
    "object-property-newline":[0],
    "allowMultiplePropertiesPerLine":[0],
    "linebreak-style": ["off"],
    "max-len": ["warn",200],
    "no-useless-concat":[2],
    "generator-star-spacing": [0],
    "consistent-return": [0],
    "react/forbid-prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "global-require": [1],
    "import/prefer-default-export": [0],
    "react/jsx-no-bind": [0],
    "react/prop-types": [0],
    "react/prefer-stateless-function": [0],
    "no-else-return": [0],
    "no-restricted-syntax": [0],
    "import/no-extraneous-dependencies": [0],
    "no-use-before-define": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-nested-ternary": [0],
    "arrow-body-style": [0],
    "import/extensions": [0],
    "no-bitwise": [0],
    "no-cond-assign": [0],
    "import/no-unresolved": [0],
    "require-yield": [1],
    "no-underscore-dangle": [2, { "allow": ["_store"] }]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "settings": {
    "import/resolver": "node"
  }
}
